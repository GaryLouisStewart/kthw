{
    "provider_vars": {
        "region": "eu-west-2",
        "shared_creds_file": "~/.aws/credentials",
        "aws_profile": "gary"
    },

    "kube_amis": {
        "master": "kthw-master",
        "worker": "kthw-worker"
    },

    "node_count": {
        "master": 2,
        "worker": 3
    },

    "kube_node_types": {
        "master": "t2.micro",
        "worker": "t2.micro"
    },

    "common_tags": {
        "Environment": "Development",
        "Platform": "Ubuntu 16.06 xenial",
        "Application": "Kubernetes cluster",
        "Service": "kubernetes",
        "Terraformed": "true",
        "Created_by": "Gary"
    },

    "vpc_vars": {
        "cidr_block": "10.0.0.15/24",
        "enable_dns_support": true,
        "enable_dns_hostnames": true,
        "enable_classiclink": false,
        "instance_tenancy": "default"
    },
    "cluster_name": "kthw",
    "aws_keypair_name": "kthw-demo",
    "kubemaster_cidr_ingress_access": ["0.0.0.0/0"],
    "kubemaster_ingress_ports": ["443"]

}
